{
	"manifest_version": 2,
	"name": "xlinkBook",
	"version": "0.0.1",
	"background": { "scripts": ["jquery-2.0.0.min.js", "background.js"] },
	"page_action": {
		"default_icon": {
			"19": "cnblogs_19.png",
			"38": "cnblogs_38.png"
		},
		"default_title": "xlinkBook", // shown in tooltip
		"default_popup": "popup.html"
	},
	"permissions" : ["tabs","http://localhost/"],
	"content_scripts":[{
		"matches": ["http://*/*"],//哪些http协议支持该插件
		//"matches":["http://www.cnblogs.com/*"],
		"js":["jquery-2.0.0.min.js", "content_script.js"]
	}]
}
